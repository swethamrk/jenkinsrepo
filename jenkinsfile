node {
    
    stage('code download') {
        git credentialsId: 'git_ks', url: 'https://github.com/swethamrk/demo.git'
            }
    stage('clean') {
        sh 'mvn clean'
            } 
	stage('compile') {             
	    sh 'mvn compile'
            } 		
    stage('package') {
        sh 'mvn package'
            } 
	 stage('deploy') {
        sh 'mvn deploy'
            } 		  	    
    }
